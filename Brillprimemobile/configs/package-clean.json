  "scripts": {
    "dev": "NODE_ENV=development tsx server/index.ts",
    "build": "npm run build:web && npm run build:server",
    "build:web": "cd client && npm run build",
    "build:server": "esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist --external:pg-native",
    "start": "NODE_ENV=production tsx server/index.ts",
    "start:prod": "NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "test-api": "node validate-endpoints.js",
    "test:comprehensive": "cd server && npx tsx test-endpoints.ts",
    "test:health": "curl -f http://0.0.0.0:5000/api/health/detailed || echo 'Server not running'",
    "validate": "npm run test:health && npm run test-api",
    "validate-endpoints": "node validate-endpoints.js",
    "test-integrations": "node test-integrations.js"
  },
